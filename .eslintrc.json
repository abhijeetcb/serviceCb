{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "radix": 0,
        "no-underscore-dangle": 0,
        "consistent-return":"off",
        "max-len":"off"
    }
}